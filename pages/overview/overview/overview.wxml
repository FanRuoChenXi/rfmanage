<!-- 分类 -->
<view style="padding: 0 16rpx">
    <t-tabs value="{{tabValue}}" bind:change="onTabChange" t-class-content="tabContent">
        <t-tab-panel label="资产" value="0">
            <view>
                <t-cell wx:for="{{assetList}}" wx:key="index" title="{{item.assetTag}}" description="名称：{{item['name'] ? tem['name'] : item['model']['name']}}" bind:click="toAssetDetail" data-id="{{item.id}}" hover arrow />
            </view>
        </t-tab-panel>
        <t-tab-panel label="许可证" value="1">
            <view>
                <t-cell wx:for="{{licensesList}}" wx:key="index" title="{{item.name}}" description="制造商：{{item['manufacturer']['name']}}" bind:click="toItemDetail" data-key="licenses" data-id="{{item.id}}" hover arrow />
            </view>
        </t-tab-panel>
        <t-tab-panel label="配件" value="2">
            <view>
                <t-cell wx:for="{{accessoryList}}" wx:key="index" title="{{item.name}}" description="制造商：{{item['manufacturer']['name']}}" data-id="{{item.id}}" hover arrow />
            </view>
        </t-tab-panel>
        <t-tab-panel label="消耗品" value="3">
            <view>
                <t-cell wx:for="{{consumableList}}" wx:key="index" title="{{item.name}}" description="制造商：{{item['manufacturer']['name']}}" data-id="{{item.id}}" hover arrow />
            </view>
        </t-tab-panel>
        <t-tab-panel label="组件" value="4">
            <view>
                <t-cell wx:for="{{componentList}}" wx:key="index" title="{{item.name}}" description="供应商：{{item['supplier']['name']}}" data-id="{{item.id}}" hover arrow />
            </view>
        </t-tab-panel>
    </t-tabs>
</view>
<!-- 底栏 -->
<tab-bar tabBarValue="overview" />